{
  "version": 2,
  "builds": [
    {
      "src": "api/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
