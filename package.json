{
  "name": "trae-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "build": "tsc -b --skipLibCheck && vite build",
    "build:frontend": "vite build",
    "build:server": "tsc api/server.ts --outDir dist-server --module esnext --target es2020 --moduleResolution node --esModuleInterop",
    "build:production": "npm run build && npm run build:server",
    "start": "node dist-server/api/server.js",
    "start:ts": "tsx api/server.ts",
    "start:production": "NODE_ENV=production node dist-server/api/server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "postinstall": "npm run build:production"
  },
  "dependencies": {
    "@google-cloud/text-to-speech": "^6.4.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.6",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.47.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^4.73.1",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "sonner": "^2.0.7",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.0.2",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76",
    "zustand": "^5.0.3",
    "nodemailer": "^6.9.15",
    "@sendgrid/mail": "^8.1.3",
    "twilio": "^5.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vercel/node": "^5.3.6",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
